@article{ WOS:000363049000007,
Author = {Sok, Serey and Yu, Xiaojiang},
Title = {Adaptation, resilience and sustainable livelihoods in the communities of
   the Lower Mekong Basin, Cambodia},
Journal = {INTERNATIONAL JOURNAL OF WATER RESOURCES DEVELOPMENT},
Year = {2015},
Volume = {31},
Number = {4},
Pages = {575-588},
Month = {OCT 2},
Abstract = {This paper analyses key contributors to sustainable livelihoods in the
   Lower Mekong Basin (LMB), Cambodia, by focusing upon villagers' access
   to assets, adaptation to shock and stress, and their degree of
   resilience to declines in natural resources. The study reveals that
   their access to the five assets for sustainable livelihoods is limited;
   that their capacity to adapt to shock and stress is low due to floods,
   drought and high food prices; and that their resilience to declines in
   natural resources is weak. Improvement in their capacity to adapt and in
   their resilience will be influenced by the degree to which they can
   access human, physical and social assets.},
