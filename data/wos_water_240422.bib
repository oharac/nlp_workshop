@article{ WOS:000341885200002,
Author = {Epstein, Graham and Nenadovic, Mateja and Boustany, Andre},
Title = {Into the deep blue sea: Commons theory and international governance of
   Atlantic Bluefin Tuna},
Journal = {INTERNATIONAL JOURNAL OF THE COMMONS},
Year = {2014},
