@article{ WOS:000767000700001,
Author = {Kimmich, Christian and Baldwin, Elizabeth and Kellner, Elke and
   Oberlack, Christoph and Villamayor-Tomas, Sergio},
Title = {Networks of action situations: a systematic review of empirical research},
