@article{ WOS:000369550200117,
Author = {Pascoe, Sean and Tobin, Renae and Windle, Jill and Cannard, Toni and
   Marshall, Nadine and Kabir, Zobaidul and Flint, Nicole},
Title = {Developing a Social, Cultural and Economic Report Card for a Regional
   Industrial Harbour},
