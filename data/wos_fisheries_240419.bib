@article{ WOS:000668219400022,
Author = {Ziegler, Jacob P. and Jardine, Sunny L. and Jones, Stuart E. and van
   Poorten, Brett T. and Janssen, Marco A. and Solomon, Christopher T.},
Title = {Investing in the commons: transient welfare creates incentives despite
   open access},
Journal = {ECOLOGY AND SOCIETY},
Year = {2021},
Volume = {26},
Number = {2},
Month = {JUN},
Abstract = {Local users may invest in managing common pool resources, thereby
   promoting social and ecological resilience. Institutional or economic
   limits on access are regarded as essential preconditions for
   incentivizing local investments, but we show that investment incentives
   can exist even under open access. We modeled a recreational harvest
   fishery in which local or centralized managers invest in fish stocking
   to maximize social welfare. Although classic open access dissipation of
   rents occurs at equilibrium, the sluggish response of fishing effort to
   changing conditions allows welfare to accrue in transition to
   equilibrium. This transient welfare creates persistent incentives to
   invest. Empirical observations showed that stocking by local collective
   action groups occurred at rates similar to model-predicted optima, while
   centralized stocking occurred at rates greater than predicted optima.
   Our results emphasize the potential benefits of local involvement in
   managing the commons, even under conditions that were previously thought
   to preclude effective collective action.},
