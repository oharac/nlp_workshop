Title = {The role of stakeholder perceptions and institutions for marine reserve
   efficacy in the Midriff Islands Region, Gulf of California, Mexico},
Journal = {OCEAN \& COASTAL MANAGEMENT},
Year = {2018},
Volume = {162},
Number = {SI},
